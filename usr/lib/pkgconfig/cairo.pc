prefix=/usr
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: cairo
Description: Multi-platform 2D graphics library
Version: 1.14.0

Requires.private: gobject-2.0 glib-2.0 >= 2.14     pixman-1 >= 0.30.0      libpng 
Libs: -L${libdir} -lcairo
Libs.private:        -lz -lz -lfreetype   -lposix
Cflags: -I${includedir}/cairo
