#!/bin/bash

qemu-img convert -f raw -O vmdk aplus.img aplus.vmdk

touch vbox.log
tail -f vbox.log &

VBoxManage internalcommands sethduuid aplus.vmdk 2d9f25fb-3313-40b9-9052-6da1d8fb19d8
VBoxManage startvm aplus

while VBoxManage list runningvms | grep aplus &> /dev/null; do
    sleep 1
done

kill %1 &> /dev/null
rm aplus.vmdk
